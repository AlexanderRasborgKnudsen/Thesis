#ifndef DIFF_H_
#define DIFF_H_

class Diff
{
private:
	short difference;

public:
	Diff(void) {
		difference = 0;
	}

	short Diff::diff(short Trapez, short TrapezOld) {
		difference = abs(Trapez - TrapezOld);
		return difference;

	}
};




#endif /* DIFF_H_ */
